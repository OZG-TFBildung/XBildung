   <xs:complexType name="Code.ArtDerHZB">
      <xs:annotation>
         <xs:appinfo>
            <codeliste>
               <nameLang>Art der Hochschulzugangsberechtigung</nameLang>
               <nameKurz>Art der HZB</nameKurz>
               <nameTechnisch>ArtDerHZB-Codes</nameTechnisch>
               <kennung>urn:xbildung-de:xbildung:codeliste:artderhzb</kennung>
               <beschreibung>Diese Codeliste enthält mögliche Ausprägungen der Art einer Hochschulzugangsberechtigung.</beschreibung>
               <herausgebernameLang>XBildung</herausgebernameLang>
               <herausgebernameKurz>XBildung.de</herausgebernameKurz>
            </codeliste>
            <versionCodeliste>
               <version>0.8</version>
               <datumGueltigkeitAb>2021-06-01</datumGueltigkeitAb>
               <versionCodelistenHandbuch>1.0</versionCodelistenHandbuch>
            </versionCodeliste>
            <codelistenspalten>
               <code>
                  <spaltennameLang>Code Name</spaltennameLang>
                  <datentyp>string</datentyp>
                  <codeSpalte>true</codeSpalte>
                  <verwendung>required</verwendung>
                  <empfohleneCodeSpalte>true</empfohleneCodeSpalte>
               </code>
               <description-de-DE-1>
                  <spaltennameLang>Schulart / Prüfung</spaltennameLang>
                  <datentyp>string</datentyp>
                  <codeSpalte>false</codeSpalte>
                  <verwendung>optional</verwendung>
                  <empfohleneCodeSpalte>false</empfohleneCodeSpalte>
               </description-de-DE-1>
