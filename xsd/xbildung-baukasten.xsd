   <xs:complexType name="Code.ArtDerHZB">
      <xs:annotation>
         <xs:appinfo>
            <codeliste>
               <nameLang>Art der Hochschulzugangsberechtigung</nameLang>
               <nameKurz>Art der HZB</nameKurz>
               <nameTechnisch>ArtDerHZB-Codes</nameTechnisch>
               <kennung>urn:xbildung-de:xbildung:codeliste:artderhzb</kennung>
               <beschreibung>Art der Hochschulzugangberechtigung</beschreibung>
               <herausgebernameLang>XBildung</herausgebernameLang>
               <herausgebernameKurz>XBildung</herausgebernameKurz>
            </codeliste>
            <versionCodeliste>
               <version>0.4</version>
               <bezugsort>.</bezugsort>
               <datumGueltigkeitAb>2021-04-26</datumGueltigkeitAb>
               <versionCodelistenHandbuch>1.0</versionCodelistenHandbuch>
               <aenderungZurVorversion>.</aenderungZurVorversion>
            </versionCodeliste>
            <codelistenspalten>
               <code>
